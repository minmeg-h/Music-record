<!--検索窓を追加-->
<h3>アーティスト名で検索</h3>
<%= form_with url: search_post_musics_path, method: :get, local: true do |form| %>
<%= form.text_field :search %>
<%= form.submit 'アーティスト名検索', name: nil %>

<!--RSpotifyで検索した結果を表示させる-->
  <h1>検索結果</h1>
  <% @musics.each do |music| %>
      <div>
        <%= music.name %>
        </div>
  <% end %>
<% end %>
